<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smartqueue">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_sq"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.ViewBookingsActivity"></activity>
        <activity android:name=".Activity.ViewNotification" />
        <activity android:name=".Activity.AddNotificationActivity" />
        <activity android:name=".Activity.ViewBlocked" />
        <activity android:name=".Activity.ViewNewRequests" />
        <activity android:name=".Activity.UserViewQueue" />
        <activity android:name=".Activity.BookDetailsActivity" />
        <activity android:name=".Activity.UserViewSeatActivity" />
        <activity android:name=".Activity.UserViewSingleDoctorActivity" />
        <activity android:name=".Activity.UserDoctorListActivity" />
        <activity android:name=".Activity.UserViewDoctorsActivity" />
        <activity android:name=".Activity.UserHomeActivity" />
        <activity android:name=".Activity.DoctorGenerateQactivity" />
        <activity android:name=".Activity.DocAddSeatActivity" />
        <activity android:name=".Activity.DoctorHomeActivity" />
        <activity android:name=".Activity.IndexActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.DoctorLoginActivity" />
        <activity android:name=".Activity.DoctorRegistration" />
        <activity android:name=".Activity.AdminHomeActivity" />
        <activity android:name=".Activity.UserRegistrationActivity" />
        <activity android:name=".Activity.MainActivity" />
    </application>

</manifest>