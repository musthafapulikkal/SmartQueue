        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_toEndOf="@+id/seat4"
        android:id="@+id/seat5"
        android:text="5"
        android:layout_marginLeft="8dp"
        android:background="@drawable/seat_slot"
        android:layout_toRightOf="@+id/seat4" />
