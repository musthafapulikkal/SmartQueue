        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@+id/seat8"
        android:layout_marginTop="8dp"
        android:id="@+id/seat15"
        android:text="15"
        android:background="@drawable/seat_slot"
      />
