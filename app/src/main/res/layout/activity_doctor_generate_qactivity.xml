        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_toEndOf="@+id/seat5"
        android:layout_marginLeft="8dp"
        android:text="6"
        android:id="@+id/seat6"
        android:background="@drawable/seat_slot"
        android:layout_toRightOf="@+id/seat5" />
